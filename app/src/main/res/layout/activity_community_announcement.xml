<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_community_announcement"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shiwaixiangcun.customer.ui.activity.CommunityAnnouncementActivity">
    <include android:id="@+id/include"
        layout="@layout/top_bar_white"/>

    <ListView

        android:layout_below="@+id/include"
        android:background="@color/ui_white"
        android:id="@+id/lv_community"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="#F9F9F9"
        android:dividerHeight="1dp"
        android:scrollbars="none">

    </ListView>


</RelativeLayout>
